{
  "version": "1.0",
  "name": "QCM (4 choices)",
  "template": "{\"type\":\"Card\",\"size\":\"md\",\"children\":[{\"type\":\"Form\",\"onSubmitAction\":{\"type\":\"qcm.submit\"},\"children\":[{\"type\":\"Col\",\"gap\":3,\"children\":[{%- set _c -%},{\"type\":\"Title\",\"value\":{{ (title) | tojson }},\"size\":\"md\"}{%-for q in questions -%}{%-set index = loop.index0 -%},{\"type\":\"Col\",\"key\":{{ (q.id) | tojson }},\"gap\":2,\"padding\":{\"y\":2},\"children\":[{\"type\":\"Caption\",\"value\":{{ ((\"Question \" ~ (index + 1))) | tojson }},\"color\":\"secondary\"},{\"type\":\"Text\",\"value\":{{ (q.prompt) | tojson }},\"size\":\"sm\",\"weight\":\"semibold\"},{\"type\":\"RadioGroup\",\"name\":{{ ((\"answers.\" ~ q.id)) | tojson }},\"options\":{{ (q.choices) | tojson }},\"required\":true},{\"type\":\"Divider\"}]}{%-endfor-%},{\"type\":\"Row\",\"children\":[{\"type\":\"Spacer\"},{\"type\":\"Button\",\"submit\":true,\"label\":\"Submit\",\"style\":\"primary\"}]}{%- endset -%}{{- (_c[1:] if _c and _c[0] == ',' else _c) -}}]}]}]}",
  "jsonSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "title": {
        "type": "string"
      },
      "questions": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "prompt": {
              "type": "string"
            },
            "choices": {
              "minItems": 4,
              "maxItems": 4,
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "label": {
                    "type": "string"
                  },
                  "value": {
                    "type": "string"
                  }
                },
                "required": [
                  "label",
                  "value"
                ],
                "additionalProperties": false
              }
            }
          },
          "required": [
            "id",
            "prompt",
            "choices"
          ],
          "additionalProperties": false
        }
      }
    },
    "required": [
      "title",
      "questions"
    ],
    "additionalProperties": false
  },
  "outputJsonPreview": {
    "type": "Card",
    "size": "md",
    "children": [
      {
        "type": "Form",
        "onSubmitAction": {
          "type": "qcm.submit"
        },
        "children": [
