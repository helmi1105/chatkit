            "type": "Col",
            "gap": 3,
            "children": [
              {
                "type": "Title",
                "value": "Quick QCM",
                "size": "md"
              },
              {
                "type": "Col",
                "key": "q1",
                "gap": 2,
                "padding": {
                  "y": 2
                },
                "children": [
                  {
                    "type": "Caption",
                    "value": "Question 1",
                    "color": "secondary"
                  },
                  {
                    "type": "Text",
                    "value": "Capital of France?",
                    "size": "sm",
                    "weight": "semibold"
                  },
                  {
                    "type": "RadioGroup",
                    "name": "answers.q1",
                    "options": [
                      {
                        "label": "Paris",
                        "value": "paris"
                      },
                      {
                        "label": "Madrid",
                        "value": "madrid"
                      },
                      {
                        "label": "Rome",
                        "value": "rome"
                      },
                      {
                        "label": "Berlin",
                        "value": "berlin"
                      }
                    ],
                    "required": true
                  },
                  {
                    "type": "Divider"
                  }
                ]
              },
              {
                "type": "Col",
                "key": "q2",
                "gap": 2,
                "padding": {
                  "y": 2
                },
                "children": [
                  {
                    "type": "Caption",
                    "value": "Question 2",
                    "color": "secondary"
                  },
                  {
                    "type": "Text",
                    "value": "2 + 2 = ?",
                    "size": "sm",
                    "weight": "semibold"
                  },
                  {
                    "type": "RadioGroup",
                    "name": "answers.q2",
                    "options": [
                      {
                        "label": "3",
                        "value": "3"
                      },
                      {
                        "label": "4",
                        "value": "4"
                      },
                      {
                        "label": "5",
                        "value": "5"
                      },
                      {
                        "label": "6",
                        "value": "6"
                      }
                    ],
                    "required": true
                  },
                  {
                    "type": "Divider"
                  }
                ]
              },
              {
                "type": "Row",
                "children": [
                  {
                    "type": "Spacer"
                  },
                  {
                    "type": "Button",
                    "submit": true,
                    "label": "Submit",
                    "style": "primary"
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  },
